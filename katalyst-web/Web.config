            <add ignoreCase="true" input="{HTTP_HOST}" pattern="^katalystadvantage\.com$"/>
          </conditions>
          <action appendQueryString="false" redirectType="Permanent" type="Redirect" url="https://www.katalystadvantage.com{REQUEST_URI}"/>
        </rule>
        <rule name="Force HTTPS" stopProcessing="false">
          <match url=".*"/>
          <conditions logicalGrouping="MatchAll">
            <add input="{HTTPS}" pattern="off"/>
          </conditions>
          <action appendQueryString="false" type="Redirect" url="https://{HTTP_HOST}{REQUEST_URI}"/>
        </rule>
      </rules>
    </rewrite>
