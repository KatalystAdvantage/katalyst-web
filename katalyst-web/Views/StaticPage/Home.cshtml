@{
    List<string> ALL_PAGES = new List<string> {
        "Home",
        "Change",
        "Services",
        "Team",
        "Clients",
        "Contact"
    };
}
@if (ViewBag.Crawler)
{
    @Html.Partial($"{ViewBag.Route.View}/{ViewBag.Route.SubView}")
}
else
{
    foreach (string page in ALL_PAGES)
    {
        @Html.Partial($"{ViewBag.Route.View}/{page}")
    }
}
